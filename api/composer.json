{
  "name": "prionyx/auction",
  "type": "project",
  "license": "MIT",
  "description": "Educational project Auction",
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "require": {
    "php": "^7.4",
    "php-di/php-di": "^6.0",
    "slim/psr7": "^0.6.0",
    "slim/slim": "^4.3",
    "symfony/console": "^5.0",
    "ext-json": "*"
  },
  "require-dev": {
    "overtrue/phplint": "^1.2",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "app": "php bin/app.php --ansi",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "lint": "phplint"
  }
}